package main

import "fmt"

func main() {

	var number = [8]int{7, 9, 12, 13, 22, 29, 30, 35}
	// fmt.Println(number)
	// var num_1, num_2, num_3, num_4, num_5, num_6 int

	for {
		var input int
		fmt.Print("Enter Number :")
		fmt.Scan(&input)

		if input != 1 && input != 2 && input != 3 && input != 4 && input != 5 && input != 6 {
			fmt.Println("Invalid number")
		}

		if input == 1 {
			fmt.Println(number[0], ",", number[1])
			break
		}
		if input == 2 {
			fmt.Println(number[1], ",", number[3])
			break
		}
		if input == 3 {
			fmt.Println(number[2], ",", number[4])
			break
		}
		if input == 4 {
			fmt.Println(number[3], ",", number[5])
			break
		}
		if input == 5 {
			fmt.Println(number[4], ",", number[6])
			break
		}
		if input == 6 {
			fmt.Println(number[5], ",", number[7])
			break
		}

	}

}
